{
  "name": "racksavant-contract",
  "version": "0.1.0",
  "description": "RackSavant NFT/Giphy Platform Smart Contract",
  "main": "index.js",
  "scripts": {
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "test": "anchor test",
    "build": "anchor build",
    "deploy:localnet": "anchor deploy --provider.cluster localnet",
    "deploy:devnet": "anchor deploy --provider.cluster devnet",
    "deploy:mainnet": "anchor deploy --provider.cluster mainnet",
    "generate-types": "anchor build && anchor-client-gen target/idl/racksavant.json client/types --program-id RackSavantProgram111111111111111111111111"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@solana/web3.js": "^1.87.6",
    "@solana/spl-token": "^0.4.0"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.0",
    "mocha": "^9.0.3",
    "prettier": "^2.6.2",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/racksavant/racksavant-contract"
  },
  "keywords": [
    "solana",
    "anchor",
    "blockchain",
    "nft",
    "fashion",
    "tokenization",
    "revenue-sharing"
  ],
